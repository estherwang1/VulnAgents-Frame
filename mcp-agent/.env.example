# MCP Server Configuration
# 复制此文件为 .env 并填写实际值

# === 服务器配置 ===
MCP_HOST=
MCP_PORT=
MCP_API_KEY=

# === 路径配置 ===
MCP_AGENT_PATH=/root/mcp-agent
WORKSPACE_ROOT=/root/pentest_workspace
STAMP_STORAGE_PATH=/root/pentest_workspace/.stamps
TOOLBOX_PATH=/root/mcp-agent/toolbox

# === 执行配置 ===
COMMAND_TIMEOUT=120
MAX_OUTPUT_LENGTH=2000
MAX_SCRIPT_SIZE=102400
